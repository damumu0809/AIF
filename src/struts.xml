<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<package name="action" extends="struts-default" namespace="/">
		<action name="user_*" class="userAction" method="{1}">
			<result name="register_input">/User/register.jsp</result>
			<result name="register_input_success">/User/index.jsp</result>
			<result name="login_input">/User/login.jsp</result>
			<result name="login_success">/User/index.jsp</result>
			<result name="resetPasswd_input">/User/resetPasswd.jsp</result>
			<result name="resetPasswd_success">/User/index.jsp</result>
		</action>
		
	</package>
</struts>

